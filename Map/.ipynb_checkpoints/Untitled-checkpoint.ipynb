    "temperatures=[10,-20,-289,100]\n",
    "\n",
    "def writer(temperatures, filepath):\n",
    "    with open(filepath, 'w') as file:\n",
    "        for c in temperatures:\n",
    "            if c>-273.15:\n",
    "                f=c*9/5+32\n",
    "                file.write(str(f)+\"\\n\")\n",
    "\n",
    "writer(temperatures, \"tempsz.txt\")"
   ]
